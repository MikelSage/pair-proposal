<h1>All Users</h1>

<table class="table table-condensed">
  <tr>
    <th>Name</th>
    <th>Username</th>
    <th>Role</th>
    <th>Delete?</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.username %></td>
      <td><%= link_to user.role, edit_admin_user_path(user) %></td>
      <td><%= link_to 'Delete', admin_user_path(user), method: :delete %></td>
    </tr>
  <% end %>
</table>
