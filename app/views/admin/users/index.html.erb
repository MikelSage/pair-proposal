<h1>All Users</h1>

<table class="table table-condensed">
  <tr>
    <th>Name</th>
    <th>Username</th>
    <th>Role</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.username %></td>
      <td><%= user.role %></td>
    </tr>
  <% end %>
</table>
